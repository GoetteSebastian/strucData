{
  "name": "Piktogramm Manager",
  "productName": "Electron tutorial app",
  "version": "1.0.0",
  "description": "The Pictogram Editor provides frontent functionality to modify the pictoLibrary.json Library. ",
  "main": "pictogramEditor.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbb-design-systems/picto-library.git"
  },
  "author": "sebastian.goette@sbb.ch",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/sbb-design-systems/picto-library/issues"
  },
  "homepage": "https://github.com/sbb-design-systems/picto-library#readme",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "electron": "^14.0.1",
    "electron-packager": "^15.4.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "pictogrameditor"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
